package main;

import java.util.Scanner;

public class Main {
	public static Scanner input = new Scanner(System.in);

public static void menu(int menuType){
	if(menuType==1){
		System.out.println("Menu 1");
	}
	else if(menuType==2){
		System.out.println("Menu 2");
		
	}
}
public static void flow(int type){
	int choice;
	int Idata;
	double Ddata;
	String Sdata;
	switch(type){
	case 1 : 
		//Integer
		RedBlackTree<Integer>.Node root = null;
        RedBlackTree<Integer> redBlackTree = new RedBlackTree<Integer>();
        do{
			menu(2);
			choice = input.nextInt();
			switch(choice){
			case 1 : 
				//find
				System.out.print("Enter the Integer you want to find : ");
				Idata = input.nextInt();
				if(redBlackTree.find(root, Idata)){
					System.out.println("element found");
				}
				else{
					System.out.println("element not found");
				}
				break;
			case 2 :  
				//insert
				System.out.print("Enter the Integer you want to insert : ");
				Idata = input.nextInt();
				root = redBlackTree.insert(root, Idata);
				
				break;
			case 3 :
				//delete
				System.out.print("Enter the Integer you want to delete : ");
				Idata = input.nextInt();
				redBlackTree.delete(root, Idata);
				break;
			case 0 : 
				break;
			default :
				System.out.println("Please enter a valid input");
				break;
			}
			
			
		}while(choice!=0);
        
        break;
	case 2 :  
		//String
		break;
	case 3 :
		//Double
		break;
	case 0 : 
		break;
	default :
		System.out.println("Please enter a valid input");
		break;
	}
	
}

