import java.util.*;
import java.io.*;

class Students{
	long id;
	String name;
	double cpi;
	
	Students(long id,String name,double cpi){
		this.id = id;
		this.name = name;
		this.cpi = cpi; 
	}
	
	public long getId(){return this.id;}
	public String getName(){return this.name;}
	public double getCpi(){return this.cpi;}
}
